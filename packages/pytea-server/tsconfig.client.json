{
    "extends": "../../tsconfig.json",
    "compilerOptions": {
        "outDir": "./public/js",
        "jsx": "react",
        "allowSyntheticDefaultImports": true,
        "allowJs": true,
        "noUnusedLocals": false,
		"noImplicitAny": true,
        "strictNullChecks": true,
        "baseUrl": ".",
        "target": "es5",
        "lib": [
            "es2015", "dom"
        ],
        "paths": {
            "pyright-internal/*": [
                "../pyright-internal/src/*"
            ],
            "pytea/*": [
                "../pytea/src/*"
            ]
        }
    },
    "include": [
        "src/client/**/*"
    ],
    "exclude": [
        "node_modules"
    ]
  }