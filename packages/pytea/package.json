{
    "name": "pytea",
    "displayName": "pytea",
    "description": "PyTEA: PyTorch Tensor Error Analyzer",
    "version": "0.1.0",
    "license": "MIT",
    "author": {
        "name": "Ho Young Jhoo"
    },
    "publisher": "Seoul National University",
    "engines": {
        "node": ">=12.0.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/MerHS/pytea",
        "directory": "packages/pytea"
    },
    "scripts": {
        "build": "webpack --mode production --progress",
        "clean": "shx rm -rf ./dist ./out",
        "webpack": "webpack --mode development --progress",
        "prepack": "npm run clean && shx cp ../../README.md . && npm run build",
        "test": "cross-env NODE_DEBUG=torch_debug jest --detectOpenHandles --forceExit torch",
        "test:unit": "cross-env NODE_DEBUG=torch_debug jest --detectOpenHandles --forceExit pytea.unit"
    },
    "dependencies": {
        "assert": "^2.0.0",
        "chalk": "^4.1.0",
        "chokidar": "^3.5.1",
        "command-line-args": "^5.1.1",
        "immutable": "^4.0.0-rc.12",
        "source-map-support": "^0.5.19",
        "tmp": "^0.2.1",
        "typescript-char": "^0.0.0",
        "zeromq": "^6.0.0-beta.6"
    },
    "devDependencies": {
        "@types/chokidar": "^2.1.3",
        "@types/command-line-args": "^5.0.0",
        "@types/jest": "^26.0.15",
        "@types/node": "^12.19.14",
        "@types/tmp": "^0.2.0",
        "clean-webpack-plugin": "^3.0.0",
        "copy-webpack-plugin": "^7.0.0",
        "cross-env": "^7.0.2",
        "jest": "^26.6.3",
        "jest-junit": "^12.0.0",
        "shx": "^0.3.3",
        "ts-jest": "^26.4.4",
        "ts-loader": "^8.0.14",
        "tsconfig-paths-webpack-plugin": "^3.3.0",
        "typescript": "^4.1.3",
        "vscode-jsonrpc": "6.0.0-next.7",
        "vscode-languageserver": "7.0.0-next.11",
        "vscode-languageserver-textdocument": "^1.0.1",
        "vscode-languageserver-types": "3.16.0-next.5",
        "webpack": "^5.15.0",
        "webpack-cli": "^4.3.1"
    },
    "files": [
        "/dist"
    ],
    "main": "index.js",
    "bin": {
        "pytea": "index.js"
    }
}
