{
    "name": "vscode-tea",
    "displayName": "PyTea",
    "description": "PyTea: PyTorch Tensor Error Analyzer",
    "version": "0.1.0",
    "private": true,
    "license": "MIT",
    "author": {
        "name": "Seoul National University"
    },
    "publisher": "snu-ropas-sf",
    "repository": {
        "type": "git",
        "url": "https://github.com/ropas/pytea"
    },
    "engines": {
        "vscode": "^1.53.0"
    },
    "keywords": [
        "python"
    ],
    "categories": [
        "Programming Languages"
    ],
    "activationEvents": [
        "onLanguage:python",
        "workspaceContains:pyteaconfig.json"
    ],
    "main": "./dist/extension.js",
    "contributes": {
        "languages": [
            {
                "id": "python",
                "aliases": [
                    "Python"
                ],
                "extensions": [
                    ".py"
                ]
            }
        ],
        "commands": [
            {
                "command": "pytea.analyzefile",
                "title": "Analyze This File",
                "category": "PyTea"
            },
            {
                "command": "pytea.restartserver",
                "title": "Restart Server",
                "category": "PyTea"
            }
        ],
        "menus": {
            "editor/context": [
                {
                    "command": "pytea.analyzefile",
                    "title": "Pytea: Analyze This File",
                    "group": "Pytea",
                    "when": "editorLangId == python"
                }
            ]
        },
        "configuration": {
            "type": "object",
            "title": "PyTea",
            "properties": {
                "python.analysis.diagnosticMode": {
                    "type": "string",
                    "default": "openFilesOnly",
                    "enum": [
                        "openFilesOnly",
                        "workspace"
                    ],
                    "enumDescriptions": [
                        "Analyzes and reports errors on only open files.",
                        "Analyzes and reports errors on all files in the workspace."
                    ],
                    "scope": "resource"
                },
                "python.analysis.diagnosticSeverityOverrides": {
                    "type": "object",
                    "default": {},
                    "description": "Allows a user to override the severity levels for individual diagnostics.",
                    "scope": "resource",
                    "properties": {
                        "reportGeneralTypeIssues": {
                            "type": "string",
                            "description": "Diagnostics for general type inconsistencies, unsupported operations, argument/parameter mismatches, etc. Covers all of the basic type-checking rules not covered by other rules. Does not include syntax errors.",
                            "default": "error",
                            "enum": [
                                "none",
                                "information",
                                "warning",
                                "error"
                            ]
                        },
                        "reportPropertyTypeMismatch": {
                            "type": "string",
                            "description": "Diagnostics for property whose setter and getter have mismatched types.",
                            "default": "error",
                            "enum": [
                                "none",
                                "information",
                                "warning",
                                "error"
                            ]
                        }
                    }
                },
                "python.analysis.logLevel": {
                    "type": "string",
                    "default": "info",
                    "description": "Specifies the level of logging for the Output panel",
                    "enum": [
                        "Error",
                        "Warning",
                        "Information",
                        "Trace"
                    ]
                }
            }
        },
        "jsonValidation": [
            {
                "fileMatch": "pyrightconfig.json",
                "url": "./schemas/pyrightconfig.schema.json"
            },
            {
                "fileMatch": "mspythonconfig.json",
                "url": "./schemas/pyrightconfig.schema.json"
            }
        ]
    },
    "scripts": {
        "clean": "shx rm -rf ./dist ./out",
        "prepackage": "node ./build/renamePackage.js pyright",
        "package": "vsce package",
        "postpackage": "node ./build/renamePackage.js vscode-pyright",
        "vscode:prepublish": "node ./build/checkPackage.js && npm run clean && webpack --mode production --progress",
        "webpack": "webpack --mode development --progress",
        "webpack-dev": "npm run clean && webpack --mode development --watch --progress"
    },
    "dependencies": {
        "vscode-jsonrpc": "6.0.0-next.7",
        "vscode-languageclient": "7.0.0-next.14",
        "vscode-languageserver": "7.0.0-next.11",
        "vscode-languageserver-protocol": "^3.16.0-next.11"
    },
    "devDependencies": {
        "@types/node": "^12.19.14",
        "@types/vscode": "~1.48.0",
        "chalk": "^4.1.0",
        "clean-webpack-plugin": "^3.0.0",
        "copy-webpack-plugin": "^6.3.1",
        "detect-indent": "^6.0.0",
        "fs-extra": "^9.0.1",
        "shx": "^0.3.3",
        "ts-loader": "^8.0.11",
        "tsconfig-paths-webpack-plugin": "^3.3.0",
        "typescript": "^4.1.3",
        "vsce": "^1.83.1",
        "webpack": "^5.15.0",
        "webpack-cli": "^4.3.1"
    }
}
